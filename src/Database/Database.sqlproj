<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="MSBuild.Sdk.SqlProj/3.2.0">
  <PropertyGroup>
  <TargetFramework>net8.0</TargetFramework>
    <Name>TsqlSampleDatabase</Name>
    <DSP>Microsoft.Data.Tools.Schema.Sql.SqlAzureV12DatabaseSchemaProvider</DSP>
  <SqlServerVersion>Sql150</SqlServerVersion>
    <RootNamespace>TsqlSampleDatabase</RootNamespace>
    <DefaultCollation>SQL_Latin1_General_CP1_CI_AS</DefaultCollation>
    <ModelCollation>1033,CI</ModelCollation>
    <IncludeCompositeObjects>true</IncludeCompositeObjects>
    <ProjectGuid>{00000000-0000-0000-0000-000000000000}</ProjectGuid>
  </PropertyGroup>
  <ItemGroup>
    <None Include="Properties\publish.xml" />
  </ItemGroup>
  <ItemGroup>
    <SqlCmdVariable Include="SeedSampleData">
      <DefaultValue>true</DefaultValue>
    </SqlCmdVariable>
  </ItemGroup>
  <ItemGroup>
    <PostDeploy Include="Scripts\PostDeployment.sql" />
  </ItemGroup>
  <ItemGroup>
    <Build Include="Tables\dbo.Sample.sql" />
  </ItemGroup>
  <ItemGroup>
    <Build Include="Tables\dbo.Customer.sql" />
    <Build Include="Tables\dbo.Product.sql" />
    <Build Include="Tables\dbo.Order.sql" />
    <Build Include="Tables\dbo.OrderItem.sql" />
  </ItemGroup>
  <ItemGroup>
    <Build Include="Constraints\dbo.Order.FK_Customer.sql" />
    <Build Include="Constraints\dbo.OrderItem.FK_Order.sql" />
    <Build Include="Constraints\dbo.OrderItem.FK_Product.sql" />
  </ItemGroup>
  <ItemGroup>
    <Build Include="Indexes\dbo.Customer.IX_LastName.sql" />
    <Build Include="Indexes\dbo.Product.IX_Name.sql" />
    <Build Include="Indexes\dbo.Order.IX_CustomerId.sql" />
    <Build Include="Indexes\dbo.Order.IX_OrderDateUtc.sql" />
    <Build Include="Indexes\dbo.OrderItem.UX_Order_Product.sql" />
    <Build Include="Indexes\dbo.OrderItem.IX_ProductId.sql" />
  </ItemGroup>
</Project>